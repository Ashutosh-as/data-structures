
bool check(vector<ll> arr, vector<ll> brr, int n) {
        unordered_map<int,int> m;
        for(int i=0;i<n;i++){
            m[arr[i]]++;
            
        }
        for(int i=0;i<n;i++){
            m[brr[i]]--;
            if(m[brr[i]]==0)
            m.erase(brr[i]);
            }
        
        return m.size()==0;
}